{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "/usr/include/qt6",
                "/usr/include/qt6/QtWidgets",
                "/usr/include/qt6/QtGui",
                "/usr/include/qt6/QtCore",
                "/usr/lib/qt6/mkspecs/linux-g++"
            ],
            "defines": [
                "QT_WIDGETS_LIB",
                "QT_GUI_LIB",
                "QT_CORE_LIB",
                "_REENTRANT"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}